{
  "name": "vaccine-progress-bot",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "serve": "nodemon -e ts src/scheduler",
    "start": "run-p serve mock",
    "mock": "mocker ./twitter-mock/api.js",
    "test": "jest",
    "build": "rm -rf dist && parcel build src/handler.ts --out-file index.js --no-source-maps --target node  --bundle-node-modules && (cd dist && zip -r -D lambda.zip *)",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.11",
    "@types/node": "^14.14.22",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "jest": "^26.6.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@types/jest": "^26.0.20",
    "mocker-api": "^2.8.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4"
  }
}
